    "@openzeppelin/contracts": "^4.3.2",
    "ethers": "^5.4.7",
    "framer-motion": "^4.1.17",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
